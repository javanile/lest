{
  "name": "javanile/lest",
  "version": "0.1.0",
  "license": "MIT",
  "type": "library",
  "description": "Legacy Code Testing Framework",
  "homepage": "https://github.com/javanile/lest",
  "keywords": ["vtiger", "command-line", "automation", "testing", "tdd"],
  "bin": [
    "lest"
  ],
  "autoload": {
    "psr-4": {
      "Javanile\\Lest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Javanile\\Lest\\Tests\\": "tests",
    }
  },
  "require": {
    "ext-json": "*",
    "javanile/vtiger-client": "^0.0.24",
    "mnapoli/silly": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "javanile/http-robot": "*",
    "javanile/mysql-import": "*",
    "psr/container": "^1.0"
  }
}